<Window xmlns:my="clr-namespace:tools"  x:Class="tools.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="tools" Height="550" Width="800" WindowStartupLocation="CenterScreen">
    <Window.Resources>        
    </Window.Resources>
    <Grid>
        <TabControl>
            <TabItem Header="数据">
                <my:DataControl/>
            </TabItem>
            <TabItem Header="正则">
                <my:RegexControl/>
            </TabItem>
            <TabItem Header="批量重命名">
                <my:RenameControl/>
            </TabItem>
            <TabItem Header="代码预览">
                <my:CodeControl/>
            </TabItem>
            <TabItem Header="简单加密解密">
                <my:EncodeDecodeControl/>
            </TabItem>
            <TabItem Header="HTML转换">
                <my:HTMLConverterControl/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
